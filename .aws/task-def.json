{
  "family": "thesis-task",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": 4096,
  "memory": 16384,
  "containerDefinitions": [
    {
      "name": "thesis-container",
      "image": "090733632942.dkr.ecr.us-east-1.amazonaws.com/master-thesis:latest",
      "cpu": 4096,
      "memory": 16384,
      "essential": true,
      "command": ["python", "main.py"],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/thesis",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
